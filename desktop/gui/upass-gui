#!/usr/bin/env python3
"""
UPass GUI Launcher
"""

import os
import sys

# Get the directory containing this script
script_dir = os.path.dirname(os.path.abspath(__file__))

# Add GUI directory to Python path FIRST
sys.path.insert(0, script_dir)

# Import and run main application - import GUI main before adding CLI path
from main import gui_main

# Add CLI directory to Python path after GUI import
sys.path.insert(0, os.path.join(script_dir, '..', 'cli'))

if __name__ == '__main__':
    sys.exit(gui_main())